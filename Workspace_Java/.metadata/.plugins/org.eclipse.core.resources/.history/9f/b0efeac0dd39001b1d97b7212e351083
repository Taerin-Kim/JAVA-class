package com.test05;

public class MTest {

	public static void main(String[] args) {
		
		TV ig = new IgTV();
		System.out.println(ig.volumeUp());   // ig tv 구매
		System.out.println(ig.volumeUp());  // 1
		System.out.println(ig.volumeUp());  // 2
		System.out.println(ig.volumeUp());  // 4
		
		System.out.println(ig.volumeDown());
		System.out.println(ig.volumeDown());
		System.out.println(ig.volumeDown());
		System.out.println(ig.volumeDown());
		
		TV samsong = new SamsongTV();
		System.out.println(samsong.volumeUp());
		System.out.println(samsong.volumeUp());
		System.out.println(samsong.volumeDown());
		System.out.println(samsong.volumeDown());
		
	}
}